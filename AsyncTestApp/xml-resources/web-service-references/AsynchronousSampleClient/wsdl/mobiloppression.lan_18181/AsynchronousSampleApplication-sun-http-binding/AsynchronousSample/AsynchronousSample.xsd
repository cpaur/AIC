<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.w3.org/2001/XMLSchema" targetNamespace="http://enterprise.netbeans.org/bpel/AsynchronousSampleSchemaNamespace">

    <element name="typeA">
        <complexType>
            <sequence>
                <element maxOccurs="unbounded" minOccurs="1" name="keyword" type="xsd:string"/>
                <element name="id" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>
    <element name="typeB">
        <complexType>
            <sequence>
                <choice>
                    <element name="pdfData" type="xsd:base64Binary"/>
                    <element name="faultInfo" type="xsd:string"/>
                </choice>
                <element name="id" type="xsd:string"/>
            </sequence>
        </complexType>
    </element>
</xsd:schema>